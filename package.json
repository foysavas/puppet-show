{
  "name": "puppet-show",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec \"./bin/run_action\" app/serve",
    "start_server": "./bin/run_action app/serve",
    "test:watch": "APP_ENV=test jest --watch",
    "test": "APP_ENV=test node --async-stack-traces --harmony -r esm-wallaby -r ./init.js --experimental-vm-modules node_modules/.bin/jest --forceExit",
    "test:inspect": "APP_ENV=test node --inspect-brk --expose-gc --async-stack-traces --harmony -r esm-wallaby -r ./init.js --experimental-vm-modules node_modules/.bin/jest --runInBand --logHeapUsage"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {
      "\\.m?jsx?$": "jest-esm-transformer"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "globalTeardown": "./jest/teardown.js",
    "globalSetup": "./jest/setup.js",
    "setupFilesAfterEnv": [
      "./jest/aroundTest.js"
    ],
    "moduleNameMapper": {
      "test:/(.*)": "<rootDir>/test/$1",
      ":/(.*)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "check-disk-space": "^3.1.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "deep-keys": "^0.5.0",
    "dotenv": "^8.2.0",
    "es6-template-string": "^1.2.0",
    "esm-wallaby": "^3.2.25",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "fetch-absolute": "^1.0.0",
    "forever-monitor": "^3.0.3",
    "formdata-polyfill": "^4.0.10",
    "fs-extra": "^8.1.0",
    "js-to-formdata": "^1.2.2",
    "leaked-handles": "^5.2.0",
    "lodash": "^4.17.15",
    "mockdate": "^3.0.5",
    "module-alias": "^2.2.2",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "mz": "^2.7.0",
    "nodemon": "^2.0.14",
    "object-to-formdata": "^4.2.2",
    "parseurl": "^1.3.3",
    "pino-http": "^5.7.0",
    "pino-pretty": "^7.0.1",
    "puppeteer": "^13.0.1",
    "scmp": "^2.1.0",
    "short-uuid": "^3.1.1",
    "sqlite3": "^5.0.2",
    "stripe": "^8.127.0",
    "subscript": "^3.0.3",
    "svg64": "^1.1.0",
    "test-listen": "^1.1.0",
    "uuid": "^3.4.0",
    "uuid-validate": "0.0.3",
    "weak-napi": "^2.0.2",
    "xmldom": "^0.4.0",
    "yargs": "^15.1.0",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "jest": "^27.3.1",
    "jest-esm-transformer": "^1.0.0"
  },
  "engines": {
    "node": "16",
    "npm": "6"
  },
  "esm": {
    "cjs": {
      "dedefault": true,
      "topLevelReturn": true
    },
    "mainFields": [
      "module",
      "main"
    ],
    "mode": "auto"
  },
  "_moduleAliases": {
    ":": "src",
    "test:": "test"
  }
}
